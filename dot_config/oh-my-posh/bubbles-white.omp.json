{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#111111",
          "background": "#f1f1f1",
          "properties": {
            "time_format": "15:04:05"
          },
          "template": " \uf64f {{.CurrentDate | date .Format}} "
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "foreground": "#111111",
          "background": "#f1f1f1",
          "properties": {
            "style": "unique"
          },
          "template": " {{if eq .Path \"~\"}}\uf74b {{.UserName}}{{else}}\uf74a {{.Path}}{{end}} {{if not (.Segments.Contains \"git\")}}<#f1f1f1,transparent>\ue0b4</>{{end}}"
        },
        {
          "alias": "git",
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#111111",
          "background": "#bcbcbc",
          "background_templates": [
            "{{if or .Working.Changed .Staging.Changed}}#bcbcbc{{end}}",
            "{{if and gt .Ahead 0 gt .Behind 0}}#bcbcbc{{end}}",
            "{{if gt .Ahead 0}}#bcbcbc{{end}}",
            "{{if gt .Behind 0}}#bcbcbc{{end}}"
          ],
          "properties": {
            "branch_max_length": 20,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "git_icon": "\uf7a1",
            "github_icon": "\uf7a3",
            "branch_icon": "\ue725 ",
            "commit_icon": "\ue729 ",
            "tag_icon": "\uf9fb ",
            "rebase_icon": "\ue728 ",
            "merge_icon": "\ue727 ",
            "revert_icon": "\uf964 ",
            "no_commits_icon": "\uf595 "
          },
          "template": " {{if .UpstreamIcon}}{{.UpstreamIcon}} | {{else}}\uf7a1 | {{end}}{{.HEAD}} | {{if .BranchStatus}}{{.BranchStatus}}{{end}}{{if .Working.Changed}} | \uf044 {{.Working.String}}{{end}}{{if .Staging.Changed}} | \uf046 {{.Staging.String}}{{end}}{{if gt .StashCount 0}} | \uf692 {{.StashCount}}{{end}} "
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "foreground": "#111111",
          "background": "#f1f1f1",
          "properties": {
            "always_enabled": true
          },
          "template": " \uf608 {{.FormattedMs}} "
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#111111",
          "background": "#bcbcbc",
          "background_templates": [
            "{{if gt .Code 0}}#717171{{end}}"
          ],
          "properties": {
            "always_enabled": true
          },
          "template": " {{if gt .Code 0}}\uf7d3{{else}}\uf7d0{{end}} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:white",
          "template": "\ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": true,
            "display_mode": "files"
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:white",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "home_enabled": false,
            "display_mode": "files"
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:white",
          "template": "\uf898 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "home_enabled": false
          }
        },
        {
          "type": "npm",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:white",
          "template": " {{ .Full }} ",
          "properties": {
            "display_mode": "files",
            "home_enabled": false
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:white",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "properties": {
            "display_mode": "context",
            "fetch_version": true,
            "fetch_virtual_env": true
          }
        },
        {
          "type": "rust",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:white",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#f1f1f1",
          "template": "\u2771"
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground": "#111111",
    "template": "<#f1f1f1, transparent>\ue0b6</><#111111,#f1f1f1> \uf64f {{.Segments.Time.CurrentDate | date \"15:04:05\"}} </><#f1f1f1, transparent>\ue0b4</> <#f1f1f1, transparent>\ue0b6</><#111111,#f1f1f1> {{if eq .Segments.Path.Path \"~\"}}\uf74b {{.UserName}}{{else}}\uf74a {{.Segments.Path.Path}}{{end}} </><#f1f1f1, transparent>\ue0b4</> <#f1f1f1, transparent>\ue0b6</><#111111,#f1f1f1> \uf608 {{.Segments.Executiontime.FormattedMs}} </>{{if gt .Code 0}}<#f1f1f1,#898989>\ue0b4</><#111111,#898989> {{if gt .Code 0}}\uf7d3{{else}}\uf7d0{{end}} </><#898989,transparent>\ue0b4</>{{else}}<#f1f1f1,#bcbcbc>\ue0b4</><#111111,#bcbcbc> {{if gt .Code 0}}\uf7d3{{else}}\uf7d0{{end}} </><#bcbcbc,transparent>\ue0b4</>{{end}} <#f1f1f1>\u2771</> "
  },
  "secondary_prompt": {
    "foreground": "#f1f1f1",
    "template": "\u276f "
  },
  "palette": {
    "white": "#f1f1f1",
    "black": "#111111"
  }
}